<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:form="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
    xmlns:m="sap.m.Dialog"
    xmlns:l="sap.ui.layout"
>
    <Dialog
        id="_IDDialog"
        title="Bidding History Report"
        class="sapUiSmallMargin"
        contentHeight="35%"
        contentWidth="50%"
    >
        <Panel id="_IDGenPanel1">
            <FlexBox id="_IDGenFlexBox1" class="flex" >
                <VBox id="_IDGenVBox1" class="flex" alignItems="End" >

                    <FlexBox id="_IDGenFlexBox2" class="headerItem">
                        <Label
                            id="_IDGenLabel1"
                            class="sapUiTinyMarginTop"
                            text="Done By Ve:"
                        />
                        <Input
                            
                            value="{biddingHisReport1>/0/DoneBy}"
                            editable="false"
                        />
                    </FlexBox>

                    <FlexBox id="_IDGenFlexBox3" class="headerItem">
                        <Label
                            id="_IDGenLabel2"
                            class="sapUiTinyMarginTop"
                            text="Created By : "
                        />
                        <Input
                            id="_IDGenInput2"
                            value="{biddingHisReport1>/0/AwrdCreatedBy}"
                            editable="false"
                        />
                    </FlexBox>

                    <FlexBox id="_IDGenFlexBox4" class="headerItem" >
                        <Label
                            id="_IDGenLabel3"
                            class="sapUiTinyMarginTop"
                            text="Status : "
                        />
                        <Input
                            id="_IDGenInput3"
                            value="{biddingHisReport1>/0/Stat}"
                            editable="false"
                        />
                    </FlexBox>

                    <FlexBox id="_IDGenFlexBox5" class="headerItem">
                        <Label
                            id="_IDGenLabel4"
                            class="sapUiTinyMarginTop"
                            text="Type(Auto):"
                        />
                        <Input
                            id="_IDGenInput4"
                            value="{biddingHisReport1>/0/Zmode}"
                            editable="false"
                        />
                    </FlexBox>

                    <FlexBox id="_IDGenFlexBox6" class="headerItem" >
                        <Label
                            id="_IDGenLabel5"
                            class="sapUiTinyMarginTop"
                            text="Comments: "
                        />
                        <Input
                            id="_IDGenInput5"
                            value="{biddingHisReport1>/0/Zcom}"
                            editable="false"
                        />
                    </FlexBox>
                    
                </VBox>

                <VBox id="_IDGenVBox2" class="flex" alignItems="End">
                    <FlexBox id="_IDGenFlexBox9" class="headerItem" >
                        <Label
                            id="_IDGenLabel8"
                            class="sapUiTinyMarginTop"
                            text="AwardInfo: "
                        />
                        <Input
                            id="_IDGenInput8"
                            value="{biddingHisReport1>/0/Award}"
                            editable="false"
                        />
                    </FlexBox>

                    <FlexBox id="_IDGenFlexBox10" class="headerItem">
                        <Label
                            id="_IDGenLabel9"
                            class="sapUiTinyMarginTop"
                            text="Voyage Type : "
                        />
                        <Input
                            id="_IDGenInput9"
                            value="{biddingHisReport1>/0/Voyty}"
                            editable="false"
                        />
                    </FlexBox>

                    <FlexBox id="_IDGenFlexBox11" class="headerItem" >
                        <Label
                            id="_IDGenLabel10"
                            class="sapUiTinyMarginTop"
                            text=" Cargo Type: "
                        />
                        <Input
                            id="_IDGenInput10"
                            value="{biddingHisReport1>/0/Carty}"
                            editable="false"
                        />
                    </FlexBox>

                    <FlexBox id="_IDGenFlexBox12" class="headerItem">
                        <Label
                            id="_IDGenLabel11"
                            class="sapUiTinyMarginTop"
                            text=" Voyage Name: "
                        />
                        <Input
                            id="_IDGenInput11"
                            value="{biddingHisReport1>/0/Voynm}"
                            editable="false"
                        />
                    </FlexBox>

                    <FlexBox id="_IDGenFlexBox13" class="headerItem">
                        <Label
                            id="_IDGenLabel12"
                            class="sapUiTinyMarginTop"
                            text="Vessel Name : "
                        />
                        <Input
                            id="_IDGenInput12"
                            value="{biddingHisReport1>/0/Vessn}"
                            editable="false"
                        />
                    </FlexBox>
 
                </VBox>
            </FlexBox>
        </Panel>

        

        <footer>
            <OverflowToolbar id="_IDGenOverflowToolbar2">
                <ToolbarSpacer id="_IDGenToolbarSpacer2" />
                <Button
                    id="_IDGenButto"
                    text="Close"
                    press="oncancell"
                />
            </OverflowToolbar>
        </footer>
    </Dialog>
</core:FragmentDefinition>
