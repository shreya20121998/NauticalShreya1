<core:FragmentDefinition xmlns="sap.m" xmlns:form="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
    xmlns:m="sap.m.Dialog"
    xmlns:l="sap.ui.layout">
    <Dialog id="_IDDialog"  title="Bidding History Report" class="sapUiSmallMargin" contentHeight="300px" contentWidth="50%" >

        <Panel>
            <form:SimpleForm editable="false" layout="ResponsiveGridLayout" >
                <Label class="sapUiTinyMarginTop" text="DoneBy Ve" />
                <Text text="{biddingHisReport1>/0/DoneBy}" />                    
                <Label class="sapUiTinyMarginTop" text="CreatedBy" />
                <Text text="{biddingHisReport1>/0/AwrdCreatedBy}"  />                    
                <Label class="sapUiTinyMarginTop" text="Status" />
                <Text text="{biddingHisReport1>/0/Stat}"  />
                <Label class="sapUiTinyMarginTop" text="Type(Auto)" />
                <Text text="{biddingHisReport1>/0/Zmode}"  />                    
                <Label class="sapUiTinyMarginTop" text="Comments" />
                <Text text="{biddingHisReport1>/0/Zcom}"  />                    
                <Label class="sapUiTinyMarginTop" text="AwardInfo" />
                <Text text="{biddingHisReport1>/0/Award}"  />                    
                <Label class="sapUiTinyMarginTop" text="Voyage Type" />
                <Text text="{biddingHisReport1>/0/Voyty}" />                    
                <Label class="sapUiTinyMarginTop" text="Cargo Type" />
                <Text text="{biddingHisReport1>/0/Carty}"  />                    
                <Label class="sapUiTinyMarginTop" text="Voyage Name" />
                <Text text="{biddingHisReport1>/0/Voynm}"  />                    
                <Label class="sapUiTinyMarginTop" text="Vessel Name" />
                <Text text="{biddingHisReport1>/0/Vessn}"  />   
            </form:SimpleForm>             
        </Panel>

        <footer>
            <OverflowToolbar id="_IDGenOverflowToolbar2">
                <ToolbarSpacer id="_IDGenToolbarSpacer2" />
                <Button id="_IDGenButto" text="Close" press="oncancell" />
            </OverflowToolbar>
        </footer>
    </Dialog>
</core:FragmentDefinition>
