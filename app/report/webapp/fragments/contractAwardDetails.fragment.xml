<core:FragmentDefinition xmlns="sap.m" xmlns:form="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
    xmlns:m="sap.m.Dialog"
    xmlns:l="sap.ui.layout">
    <Dialog id="_IDDialog"  title="Contract Award Detail" class="sapUiSmallMargin" contentHeight="300px" contentWidth="50%" >

        <Panel>
            <form:SimpleForm editable="false" layout="ResponsiveGridLayout" >
                <Label class="sapUiTinyMarginTop" text="DoneBy Ve" />
                <Text text="{contractAwardReport1>/0/DoneBy}" />                    
                <Label class="sapUiTinyMarginTop" text="CreatedBy" />
                <Text text="{contractAwardReport1>/0/Lifnr}"  />                    
                <Label class="sapUiTinyMarginTop" text="Status" />
                <Text text="{contractAwardReport1>/0/Stat}"  />
                <Label class="sapUiTinyMarginTop" text="Type(Auto)" />
                <Text text="{contractAwardReport1>/0/Zmode}"  />                    
                <Label class="sapUiTinyMarginTop" text="Comments" />
                <Text text="{contractAwardReport1>/0/Zcom}"  />                    
                <Label class="sapUiTinyMarginTop" text="Award Created By" />
                <Text text="{contractAwardReport1>/0/AwrdCreatedBy}" />                    
                <Label class="sapUiTinyMarginTop" text="Award Created At" />
                <Text text="{contractAwardReport1>/0/AwrdCreatedAt}"  />                    
                <Label class="sapUiTinyMarginTop" text="Award Created On" />
                <Text text="{contractAwardReport1>/0/AwrdCreatedOn}"  />

            </form:SimpleForm>             
        </Panel>

        <footer>
            <OverflowToolbar id="_IDGenOverflowToolbar2">
                <ToolbarSpacer id="_IDGenToolbarSpacer2" />
                <Button id="_IDGenButto" text="Close" press="oncancell" />
            </OverflowToolbar>
        </footer>
    </Dialog>
</core:FragmentDefinition>
